#!/usr/bin/env ruby
# frozen_string_literal: true

require_relative '../newsrc/blog'

SKIP_THESE = [
  'data',
  'newsrc',
  'old',
  'scripts',
  'spec',
  'src',
  'www'
]

def static_directories
  Blog::Files.directories.reject { |d| SKIP_THESE.include?(File.basename(d)) }
end

puts static_directories.join(' ')
