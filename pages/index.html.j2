<h2>Latest Entry</h2>
{{ latest | embed }}

<h2>Pages</h2>
<table>
  {%- for page in pages %}
    {%- if page.filename != "index.html" %}
      <tr>
        <td>
          <a href="./{{ page.filename }}">{{ page.filename }}</a>
        </td>
        <td>{{ page.description }}</td>
      </tr>
    {%- endif %}
  {%- endfor %}
</table>
